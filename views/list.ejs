<%- include('partials/header.ejs') %>
<div class="container">
    <!--top part-->
    <div class="row">
      <div class="col"><!--spacing--></div>

      <div class="col-3">
        <br />
        <h1 class="bahnschrift"><%= tabTitle %></h1>
      </div>
      <div class="col-3"><!--spacing--></div>
      <div class="col-4">
          <div class="d-flex flex-row bd-highlight justify-content-end">
              <button type="button" class="btn misc">
                  <img src="img/refresh.png">
              </button>
              <button type="button" class="btn me-3 misc">
                  <img src="img/more.png">
              </button>
              <button type="button" id="sendBtn" class="bahnschrift btn btn-primary"><%= proceedBtn %></button>
            </div>
      </div>
      <div class="col"><!--spacing--></div>
    </div>
    <br>
    <table class="table">
      <thead>
        <tr>
          <th scope="col">#</th>
          <th scope="col">Order No.</th>
          <th scope="col">Created</th>
          <th scope="col">Time Elapsed</th>
          <th scope="col">Customer name</th>
          <th scope="col-2"><!--detail space--></th>
          <th scope="col">Price</th>
        </tr>
      </thead>
      <tbody>
        
        <% for (var i= 0; i< newListItems.length; i++){ %>
        <tr class="cardElement">
          <!--checkbox-->
          <th scope="row">[]</th>
          <td><%= newListItems[i].number%></td>
          <td><%= newListItems[i].time%></td>
          <td>TimeElapsedPlaceholder</td>
          <td><%= newListItems[i].name%></td>
          <td><a href="">Details</a></td>
          <td><%= newListItems[i].price%></td>
        </tr>
        <% } %>
      </tbody>
    </table>
  </div>

  <script
    src="https://code.jquery.com/jquery-3.6.4.min.js"
    integrity="sha256-oP6HI9z1XaZNBrJURtCoUT5SUnxFr8s3BzRl+cbzUq8="
    crossorigin="anonymous"
  ></script>
  <script
    src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha2/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-qKXV1j0HvMUeCBQ+QVp7JcfGl760yU08IQ+GpUo5hlbpg51QRiuqHAJz8+BrxE/N"
    crossorigin="anonymous"
  ></script>
  <script src="index.js"></script>
</body>
</html>
